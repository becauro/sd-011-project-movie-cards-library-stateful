{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": "trybe-frontend",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off"
  },
  "plugins": ["react", "sonarjs", "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"]
}
